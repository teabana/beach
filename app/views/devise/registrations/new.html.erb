<div id="login-box">
  <div class="left">
    <h2>Sign up</h2>

    <%= form_with model: @user, url: user_registration_path, class: registration_main, local: true do |f| %>

    <%# <%= render "devise/shared/error_messages", resource: resource %>

      <div class="form-group">
        <%= f.text_area :nickname, class: "input-default", id:"nickname", placeholder: "Username", maxlength:"40" %>
      </div>
      <div class="form-group">
        <%= f.email_field :email, class:"input-default", id:"email", placeholder: "E-mail", autofocus: true %>
      </div>
      <div class="form-group">
        <%= f.password_field :password, class:"input-default", id:"password", placeholder: "Password" %>
      </div>
      <div class="form-group">
        <%= f.password_field :password_confirmation, class:"input-default", id:"password_confirmation", placeholder: "Retype password" %>
      </div>
      <div class="actions">
        <%= f.submit "Sign me up" %>
      </div>
    <% end %>
  </div>
  <div class="right">
    <span class="loginwith">Sign in with<br />social network</span>

    <button class="social-signin facebook">Log in with facebook</button>
    <button class="social-signin twitter">Log in with Twitter</button>
    <button class="social-signin google">Log in with Google+</button>

    <%# <%= render "devise/shared/links" %>
  </div>
  <div class="or">OR</div>
<div>
